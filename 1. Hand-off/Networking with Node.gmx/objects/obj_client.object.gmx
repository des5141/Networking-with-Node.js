            <kind>1</kind>
            <string>/// Moving
if(control == obj_network.login_uuid)
{
    var temp_x = x, temp_y = y;
    temp_x += ((keyboard_check(vk_left)*-1)+keyboard_check(vk_right))*5;
    temp_y += ((keyboard_check(vk_up)*-1)+keyboard_check(vk_down))*5;
    temp_x = clamp(temp_x, 0, room_width);
    temp_y = clamp(temp_y, 0, room_height);

    if(keyboard_check(vk_left)+keyboard_check(vk_right)+keyboard_check(vk_up)+keyboard_check(vk_down) &gt; 0)
    {
        //Construct message
        var json_data = ds_map_create();
        ds_map_add(json_data, "id", NN.outsig_user_position);
        ds_map_add(json_data, "msg", "");
        ds_map_add(json_data, "user_x", temp_x);
        ds_map_add(json_data, "user_y", temp_y);
        ds_map_add(json_data, "user_uuid", obj_network.login_uuid);
        var body = json_encode(json_data);
        ds_map_destroy(json_data);
        
        //Send message
        nn_send_message(body);
    }
    
    //View setting
    //obj_view.target_x = x;
    //obj_view.target_y = y;
}else{
    x += (target_x - x)/2;
    y += (target_y - y)/2;
}
</string>
