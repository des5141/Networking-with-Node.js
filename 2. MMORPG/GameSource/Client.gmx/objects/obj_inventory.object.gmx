            draw_set_color(c_white)
            draw_set_font(fnt_inventory)
            draw_set_valign(fa_bottom)
            /*if(inventory_cursor &gt;= 0)
            {*/
                draw_text(view_xview[0]+315, view_yview[0]+390, "사용")
                draw_text(view_xview[0]+385, view_yview[0]+390, "정보")
                draw_text(view_xview[0]+455, view_yview[0]+390, "버림")
            //}
            draw_set_valign(fa_top)
            draw_set_font(fnt_inventory)
            for(i = 0; i &lt; 14; i++)
            {
                if not(item[i] = 0)
                {
                    draw_set_color(item_color(item[i]))
                    draw_text(view_xview[0]+240, view_yview[0]+45 + (i * 23), item_name(item[i]))
                }
            }
        }
        
        if(inventory_item == "item")and(inventory_cursor &gt;= 0)
        {
            draw_set_color(c_red)
            draw_rectangle(view_xview[0]+225, view_yview[0]+46 + (inventory_cursor*23), view_xview[0]+225+5, view_yview[0]+46+20 + (inventory_cursor*23), false)
            
            for(i = 0; i &lt; 14; i++)
            {
                if(item[i] = 0)and(i &lt;= inventory_cursor)
                {
                    inventory_cursor = i-1;
                }
            }
        }
        
        if(inventory_item == "select")and(inventory_cursor &gt;= 0)
        {
            draw_set_color(c_red)
            draw_rectangle(view_xview[0]+300 + (inventory_cursor*70), view_yview[0]+390+3, view_xview[0]+300+5 + (inventory_cursor*70), view_yview[0]+390-15-6, false)
        }
        
