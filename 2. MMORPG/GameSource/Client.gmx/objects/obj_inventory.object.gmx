    draw_set_halign(fa_left);
    draw_set_font(fnt_inventory)
    draw_text(view_xview[0]+15 + 10, view_yview[0]+67, "Lv " + string(global.level))
    draw_text(view_xview[0]+15 + 10, view_yview[0]+90, "HP "+ string(global.hp) + "/" + string(20 + 5 * (global.level - 1) ))
    draw_text(view_xview[0]+15 + 10, view_yview[0]+110, "G " + string(global.gold))
    
