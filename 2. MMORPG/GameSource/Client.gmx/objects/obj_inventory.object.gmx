    {
        draw_set_color(c_red)
        draw_rectangle(view_xview[0]+20 + 6, view_yview[0]+150 + (inventory_cursor*25), view_xview[0]+25 + 6, view_yview[0]+150+25 + (inventory_cursor*25), false)
    }
    
