    <event eventtype="10" enumb="13">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if not(instance_exists(obj_dialoguer))and(global.hp &gt;= 0)
{
    if(inventory)and(inventory_item = "select")and(inventory_cursor &gt;= 0)and(delay &gt; 25)
    {
        delay = 0;
        switch(inventory_cursor)
        {
            case 0:
                inventory_use(inventory_select_item);
                inventory_sort();
                inventory_item = "item"
                if(item[inventory_select_item] = 0)
                {
                    inventory_select_item = 0;
                }
                inventory_cursor = inventory_select_item;
                break;
        
            case 1:
                inventory_info(inventory_select_item);
                inventory_sort();
                inventory_item = "item"
                inventory_cursor = inventory_select_item;
                break;
                
            case 2:
                inventory_drop(inventory_select_item);
                inventory_sort();
                inventory_item = "item"
                inventory_cursor = inventory_select_item;
                break;
        }
        
    }else
    if(inventory)and(inventory_item = "item")and(inventory_cursor &gt;= 0)and(delay &gt; 25)
    {
        inventory_item = "select"
        inventory_select_item = inventory_cursor
        inventory_cursor = 0
    }
    
    if(inventory)and(inventory_item = "")and(inventory_cursor &gt;= 0)and(delay &gt; 25)
    {
        inventory_select_town = inventory_cursor;
        switch(inventory_cursor)
        {
            case 0:
                inventory_item = "item"
                inventory_cursor = 0
                break;
                
            case 1:
                inventory_item = "state"
                inventory_cursor = 0
                break;
                
            case 2:
                inventory_item = "skill"
                inventory_cursor = 0
                break;
        }
    }
}
</string>
